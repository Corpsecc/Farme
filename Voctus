Musica:AddTextbox({
	Name = "Som",
	Default = "Id",
	TextDisappear = true,
	Callback = function(id)
		for i = 1,2 do
			workspace.Main.WhistleNotePlayed:FireServer(0, game.Players.LocalPlayer.Character.HumanoidRootPart, id)
		end
		if game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild('Sound') then
			game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild('Sound').SoundId = 'rbxassetid://'..id..''
