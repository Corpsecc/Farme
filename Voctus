getgenv().DNoclip = game:GetService("RunService").Stepped:Connect(Noclip)

getgenv().DAnti_Flings = game:GetService("RunService").Stepped:Connect(Anti_Fling)

getgenv().Entregador = false

getgenv().Anti_Afk = false
getgenv().Anti_lag = false
getgenv().idled = game:GetService("Players").LocalPlayer.Idled:connect(function()
    game:GetService("VirtualUser"):ClickButton2(Vector2.new())
end)

getgenv().Name_Rainbow = false
getgenv().Song = false
