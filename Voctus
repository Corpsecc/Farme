		local Raib = math.random(#House)
		local nameH = tostring(game.Players.LocalPlayer.House.Value)
		local Houses = workspace.Houses[nameH]
		for _,Upgrades in pairs(Houses[tostring(Houses.CurrentUpgrade.Value)]:GetChildren()) do
			if Upgrades:IsA("Part") then
				game.ReplicatedStorage.HouseChannel:FireServer("PaintHouse", Upgrades, "ForceField", House[Raib])
			end
		end
	end
end
